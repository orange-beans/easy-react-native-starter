{
  "name": "easy-react-native-starter",
  "version": "0.1.0",
  "author": "orange-beans",
  "license": "MIT",
  "keywords": [
    "javascript",
    "android",
    "ios",
    "mobile",
    "react",
    "react-native",
    "starter-kit",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/orange-beans/easy-react-native-starter.git"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android-start": "cd node_modules/react-native/packager/ && packager.sh",
    "android-set-port": "adb reverse tcp:8081 tcp:8081",
    "android-build-debug": "cd android && gradlew assembleDebug",
    "android-build-release": "cd android && gradlew assembleRelease",
    "android-install-debug": "adb install android/app/build/outputs/apk/app-debug.apk",
    "android-install-release": "adb install android/app/build/outputs/apk/app-release.apk",
    "a": "cd node_modules/react-native/packager/ && packager.sh",
    "a-s-p": "adb reverse tcp:8081 tcp:8081",
    "a-b-d": "cd android && gradlew assembleDebug",
    "a-b-r": "cd android && gradlew assembleRelease",
    "a-i-d": "adb install android/app/build/outputs/apk/app-debug.apk",
    "a-i-r": "adb install android/app/build/outputs/apk/app-release.apk"
  },
  "dependencies": {
    "apsl-react-native-button": "^3.0.0",
    "babel-eslint": "^6.1.2",
    "eslint": "^3.2.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.0.0",
    "react": "15.2.1",
    "react-native": "0.31.0",
    "react-native-material-kit": "^0.3.2",
    "react-native-router-flux": "^3.34.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
