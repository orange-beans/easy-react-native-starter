{
  "name": "ScalableReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
		
		"start-android" : "cd node_modules/react-native/packager/ && packager.sh",
    "set-android-port" : "adb reverse tcp:8081 tcp:8081",
    "build-android-debug": "cd androidroid && gradlew assembleDebug",
    "build-android-release": "cd androidroid && gradlew assembleRelease",
    "install-android-debug": "adb install androidroid/app/build/outputs/apk/app-debug.apk",
    "install-android-release": "adb install androidroid/app/build/outputs/apk/app-release.apk"
  },
  "dependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.2.2",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.0.0",
    "react": "15.2.1",
    "react-native": "0.31.0",
    "react-native-router-flux": "^3.34.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
